{
  "identifier": "default",
  "description": "some permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:allow-app-write",
    "fs:allow-app-meta",
    "fs:allow-read-text-file",
    "sql:allow-load",
    "sql:default",
    "sql:allow-execute",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "$APPDATA/**"
        }
      ]
    },
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA/**"
        },
        {
          "path": "$APPCONFIG/**"
        }
      ]
    },
    "os:default",
    "dialog:default",
    "process:allow-restart"
  ]
}
