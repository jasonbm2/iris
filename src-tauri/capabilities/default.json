{
  "identifier": "default",
  "description": "some permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:allow-app-write",
    "fs:allow-app-meta",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA/user/*"
        },
        {
          "path": "$APPDATA/user/recordings/*"
        },
        {
          "path": "$APPDATA\\user\\*"
        },
        {
          "path": "$APPDATA\\user\\recordings\\*"
        }
      ]
    },
    "os:default",
    "dialog:default",
    "process:allow-restart"
  ]
}
