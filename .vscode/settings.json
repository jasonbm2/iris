{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "files.associations": {
    "LICENSE": "plaintext"
  },
  "[markdown]": {
    "editor.rulers": [80],
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[plaintext]": {
    "editor.rulers": [80],
    "editor.wordWrap": "off"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[javascript][typescript][javascriptreact][typescriptreact][json][css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "APPCONFIG",
    "choosealicense",
    "consts",
    "datalabels",
    "Hasher",
    "lcov",
    "Meds",
    "Ojos",
    "ojosproject",
    "Palliaview",
    "Pipenv",
    "rusqlite",
    "struct",
    "structs",
    "Tauri",
    "webm"
  ]
}
